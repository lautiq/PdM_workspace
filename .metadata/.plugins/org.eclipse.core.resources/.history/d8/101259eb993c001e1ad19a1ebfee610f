#ifndef API_INC_API_MEF_H_
#define API_INC_API_MEF_H_


typedef enum
{
	STATE_INIT,
	STATE_START,
	STATE_PAUSE,
	STATE_RESUME,
	STATE_STOP
}FSM_State;

//Function declarations

void MEF_Init();
void MEF_Update();
FSM_State MEF_GetState();
void MEF_EnterState(FSM_State state);
void MEF_ExitState();




#endif /* API_INC_API_MEF_H_ */
